{
  "manifest_version": 3,
  "name": "Edge",
  "version": "1.0",
  "author": "Mace",
  "description": "The only chrome extension you need.",
  "permissions": ["storage"],
  "host_permissions": ["<all_urls>"],
  "chrome_url_overrides": {
    "newtab": "homepage/homepage.html"
  },
  "action": {
    "default_popup": "popup/popup.html"
  }
}
